   "source": [
    "#rewriting to increase fidelity. The lower thresh hold makes sure it doesn't make up and thing that it cant see on it's flanks\n",
    "def re_find_ubstruction(img, upper_thresh=(140, 140, 140), lower_thresh=(1, 1, 1)):\n",
    "    # Create an array of zeros same xy size as img, but single channel\n",
    "    color_select = np.zeros_like(img[:,:,0])\n",
    "    # Require that each pixel be above all three threshold values in RGB\n",
    "    # between_thresh will now contain a boolean array with \"True\"\n",
    "    # where threshold was met\n",
    "    between_thresh = (img[:,:,0] < upper_thresh[0]) \\\n",
